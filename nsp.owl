    <!-- http://www.normansicily.org/nsp#contains -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#contains">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isIn"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Province"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:comment xml:lang="en">A province (administrative grouping) contains a location</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#hasChildren -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#hasChildren">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isChildOf"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:comment xml:lang="en">A Monastery has one or more Monasteries subject to it</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#hasFounder -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#hasFounder">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isFounderOf"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Place"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Founder"/>
        <rdfs:comment xml:lang="en">A Place has one or more founders</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#hasLocation -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#hasLocation">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isLocationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Name"/>
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:comment xml:lang="en">A Name or a Place has a known Location</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#hasName -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#hasName">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isNameOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Location"/>
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Name"/>
        <rdfs:comment xml:lang="en">A Location or Place has Name</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#hasParent -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#hasParent">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#isParentOf"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:comment xml:lang="en">A Monastery has a Motherhouse</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isChildOf -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isChildOf">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:comment xml:lang="en">A Monastery is subject to another Monastery</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isFounderOf -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isFounderOf">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Founder"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Place"/>
        <rdfs:comment xml:lang="en">A Founder (Person) founded a Place</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isIn -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isIn">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Province"/>
        <rdfs:comment xml:lang="en">A location is contained in a province (administrative grouping)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isLocationOf -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isLocationOf">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Name"/>
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A Location is known for a Name or Place</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isNameOf -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isNameOf">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Name"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Location"/>
                    <rdf:Description rdf:about="http://www.normansicily.org/nsp#Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A Name is known for a Location or Place</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isNear -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isNear">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Municipality"/>
        <rdfs:comment xml:lang="en">A Location is nearby, or contained in, a Municipality</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#isParentOf -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#isParentOf">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Monastery"/>
        <rdfs:comment xml:lang="en">A Monastery is the motherhouse to another Monastery</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#wasContainedBy -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#wasContainedBy">
        <owl:inverseOf rdf:resource="http://www.normansicily.org/nsp#wasIn"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/HistoricalProvince"/>
        <rdfs:range rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:comment xml:lang="en">A historical province contains a Location</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


    <!-- http://www.normansicily.org/nsp#wasIn -->

    <owl:ObjectProperty rdf:about="http://www.normansicily.org/nsp#wasIn">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/HistoricalProvince"/>
        <rdfs:comment xml:lang="en">A Location was contained in a historical province</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:ObjectProperty>
    


