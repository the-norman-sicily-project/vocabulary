    <owl:DatatypeProperty rdf:about="http://www.normansicily.org/nsp#maxEarliestAttestation">
        <rdfs:subPropertyOf rdf:resource="http://www.normansicily.org/nsp#earliestAttestation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment xml:lang="en">The latest year an attestation could have been made</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
