    </owl:DatatypeProperty>
    


    <!-- http://www.normansicily.org/nsp#earliestAttestation -->

    <owl:DatatypeProperty rdf:about="http://www.normansicily.org/nsp#earliestAttestation">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Name"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
