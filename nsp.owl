        <rdfs:comment xml:lang="en">An order is a group of person living under a religious rule</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.normansicily.org/nsp#placeCategory -->

