    </owl:DatatypeProperty>
    


    <!-- http://www.normansicily.org/nsp#isPreferred -->

    <owl:DatatypeProperty rdf:about="http://www.normansicily.org/nsp#isPreferred">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Name"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">If true, then it denotes that the name is the preferred one for the Place or Location</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.normansicily.org/nsp#locationCategory -->

    <owl:DatatypeProperty rdf:about="http://www.normansicily.org/nsp#locationCategory">
        <rdfs:domain rdf:resource="http://www.normansicily.org/nsp#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Associated Modern</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Central Point</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Representative</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Unknown</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:isDefinedBy rdf:resource="http://www.normansicily.org/nsp#"/>
        <rdfs:seeAlso rdf:resource="https://pleiades.stoa.org/vocabularies/location-types"/>
    </owl:DatatypeProperty>
    


